Project Apache {
    Roles: Developer, Committer, PMCMember, PMCChair
    Deadlines:
        twoDays : 60
    Rules:
        noCodeRule : Majority {
            applied to TASK "(tag != code)"
            when TASK_REVIEW
            people Developer, Committer,
            PMCMember, PMCChair
            range PRESENT
            min votes 2
            deadline twoDays
        }
}
